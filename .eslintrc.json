{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "google",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "max-len": "off",
    "indent": ["error", 2]
  }
}
